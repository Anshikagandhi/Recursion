/*

Sample Input 1 :
1231
Sample Output 1:
1231

*/


#include <math.h>
int stringToNumber(char input[]) {
len=0;
  for(int i=0;input[i]!=0;i++)
   len++;
  
  if(len==1)
  {
    int a = input[0]-48;
    return a;
  }
  int b= stringToNumber(input+1);
  int ans= input[0]-48;
  int answer=ans*pow(10,len-1)+b;
  return answer;
  
}


